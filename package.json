{
  "name": "agdq16-layouts",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/GamesDoneQuick/agdq16-layouts.git"
  },
  "homepage": "http://gamesdonequick.com/",
  "contributors": [
    "Alex Van Camp <email@alexvan.camp>",
    "Chris Hanel <chrishanel@gmail.com>"
  ],
  "description": "The on-stream graphics used during Awesome Games Done Quick 2016.",
  "license": "Apache-2.0",
  "nodecg": {
    "compatibleRange": "~0.7.0",
    "dashboardPanels": [
      {
        "name": "schedule",
        "title": "Schedule",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "schedule.html"
      },
      {
        "name": "edit-current-run",
        "title": "Edit Current Run",
        "width": 4,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-current-run.html"
      },
      {
        "name": "advertisements",
        "title": "Advertisements",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "advertisements.html"
      },
      {
        "name": "checklist",
        "title": "Checklist",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "checklist.html"
      },
      {
        "name": "omnibar",
        "title": "Omnibar",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "omnibar.html"
      },
      {
        "name": "total",
        "title": "Total",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "total.html"
      },
      {
        "name": "edit-total",
        "title": "Edit Total",
        "width": 2,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-total.html"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      },
      {
        "file": "custom_controls/stopwatches/index.html",
        "width": 1280,
        "height": 720
      }
    ]
  },
  "dependencies": {
    "clone": "^1.0.2",
    "debounce": "^1.0.0",
    "deep-equal": "^1.0.1",
    "md5-file": "^2.0.4",
    "numeral": "^1.5.3",
    "request": "^2.67.0",
    "rieussec": "^0.2.4"
  }
}
